{
  "name": "tablesrenderapp",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://large-table-render-virtualize.netlify.app/",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.30",
    "@fortawesome/free-solid-svg-icons": "^5.14.0",
    "@fortawesome/react-fontawesome": "^0.1.11",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.1.2",
    "crypto-js": "^4.0.0",
    "highcharts": "^8.2.0",
    "highcharts-react-official": "^3.0.0",
    "isomorphic-fetch": "^2.2.1",
    "jest-styled-components": "^7.0.3",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-base-table": "^1.11.3",
    "react-dom": "^16.13.1",
    "react-redux": "^7.1.3",
    "react-scripts": "3.4.3",
    "react-spinners": "^0.9.0",
    "react-tooltip": "^4.2.9",
    "react-virtualized-auto-sizer": "1.0.2",
    "react-window": "1.8.5",
    "redux": "^4.0.5",
    "redux-saga": "^1.1.3",
    "styled-components": "^5.1.1",
    "redux-logger": "^3.0.6"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.+(js|jsx)": [
      "eslint --fix",
      "git add"
    ],
    "src/**/*.+(json|css|md)": [
      "prettier --write",
      "git add"
    ]
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint .",
    "format": "prettier --write 'src/**/*.+(json|css|md)'",
    "predeploy": "npm run cleanghpages && npx react-scripts build --profile",
    "cleanghpages": "gh-pages-clean",
    "deploy": "gh-pages -d build"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "eslint": "^7.8.1",
    "eslint-plugin-jest": "^24.0.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.0",
    "gh-pages": "^3.1.0",
    "husky": "^4.3.0",
    "lint-staged": "^10.3.0",
    "prettier": "^2.1.1",
    "react-test-renderer": "^16.13.1",
    "redux-mock-store": "^1.5.4"
  }
}
